<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/nearTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20ssp"
            android:layout_marginTop="@dimen/_10ssp"
            android:drawableStart="@drawable/marker_yello_ic"
            android:drawablePadding="@dimen/_10ssp"
            android:fontFamily="@font/proximanova_reg"
            android:gravity="center|start"
            android:text="title"
            android:textColor="@color/text_whitegray"
            android:textSize="@dimen/_12ssp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="@color/text_yello"
            app:layout_constraintEnd_toEndOf="@+id/nearTitle"
            app:layout_constraintStart_toStartOf="@+id/nearTitle"
            app:layout_constraintTop_toBottomOf="@+id/nearTitle" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>